@model IEnumerable<CircoParrotTools.Web.Models.JobMediaConvert>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<h1>Lista de Jobs</h1>
<!--<table class="table table-bordered table-striped" style="clear: both">-->
<table class="table table-bordered table-striped" style="clear: both">
    <tr>
        <th>Id Job</th>
        <th>File to Convert</th>
        <th>Time Start</th>
        <th>Time Finish</th>
        <th>Queue</th>
        <th>Status</th>
    </tr>

    @foreach (var jobMC in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => jobMC.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => jobMC.FileToConvert)
    </td>
    <td>
        @Html.DisplayFor(modelItem => jobMC.TimeStart)
    </td>
    <td>
        @Html.DisplayFor(modelItem => jobMC.TimeFinish)
    </td>
    <td>
        @Html.DisplayFor(modelItem => jobMC.Queue)
    </td>
    <td>
        @Html.DisplayFor(modelItem => jobMC.Status)
    </td>
</tr>

    }
</table>
